<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_purchase_order_inherit_form" model="ir.ui.view">
        <field name="name">view.purchase.order.inherit.form</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form" />
        <field name="arch" type="xml">
            <xpath
                expr="//page[@name='products']/field/tree/field[@name='product_qty']"
                position="after"
            >
                <field name="master_qty" optional="hide" />
                <field name="outter_qty" optional="hide" />
            </xpath>
        </field>
    </record>
</odoo>
